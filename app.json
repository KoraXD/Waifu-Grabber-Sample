{
   "name":"Waifu-Auto-Grabber",
   "description":"Waifu Auto Grabber In Pyrogram",
   "logo":"https://graph.org/file/d7cdfa2cad5f6b3c5a149.jpg",
   "keywords":[
      "telegram",
      "grabber",
      "auto",
      "userbot",
      "waifu"
   ],
   "repository":"https://github.com/KoraXD/Waifu-Auto-Grabber",
   "addons":[
      {
         "options":{
            "version":"12"
         },
         "plan":"heroku-postgresql"
      }
   ],
   "env":{
      "API_ID":{
         "description":"Get API_ID from my.telegram.org.",
         "required":true,
         "value":""
      },
      "API_HASH":{
         "description":"Get API_HASH from my.telegram.org.",
         "required":true,
         "value":""
      },
      "SESSION":{
         "description":"Your Pyrogram V2 String Session.",
         "required":true,
         "value":""
      },
      "OWNER_ID":{
         "description":"Your Telegram Account ID.",
         "required":true,
         "value":""
      },
      "MONGO_URL":{
         "description":"Your MongoDB URI, Database is Required For Waifu Auto Grabber.",
         "required":true,
         "value":""
      }
   }
           
